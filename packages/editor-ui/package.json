{
  "name": "n8n-editor-ui",
  "version": "0.169.0",
  "description": "Workflow Editor UI for n8n",
  "license": "SEE LICENSE IN LICENSE.md",
  "homepage": "https://n8n.io",
  "author": {
    "name": "Jan Oberhauser",
    "email": "jan@n8n.io"
