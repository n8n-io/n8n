{
  "name": "@n8n/design-system",
  "version": "1.77.0",
  "main": "src/index.ts",
  "import": "src/index.ts",
  "scripts": {
    "dev": "pnpm run storybook",
    "clean": "rimraf dist .turbo",
    "build": "vite build",
    "typecheck": "vue-tsc --noEmit",
