    "openapi-types": "^10.0.0",
    "p-cancelable": "^2.0.0",
    "passport": "^0.5.0",
    "passport-cookie": "^1.0.9",
    "passport-jwt": "^4.0.0",
    "pg": "^8.3.0",
    "prom-client": "^13.1.0",
    "request-promise-native": "^1.0.7",
    "sqlite3": "^5.0.2",
    "sse-channel": "^3.1.1",
    "swagger-ui-express": "^4.3.0",
    "tslib": "1.14.1",
    "typeorm": "0.2.30",
