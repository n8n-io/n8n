{
  "name": "@n8n/workflow-json-sdk",
  "type": "module",
  "version": "0.0.1",
  "files": [
    "dist"
  ],
  "exports": {
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "test": "vitest run",
    "test:dev": "vitest --silent=false",
    "lint": "eslint src --quiet",
    "lint:fix": "eslint src --fix",
    "format": "biome format --write . && prettier --write . --ignore-path ../../../../.prettierignore",
    "format:check": "biome ci . && prettier --check . --ignore-path ../../../../.prettierignore",
    "example:basic": "tsx examples/basic-workflow.ts",
    "example:ai-agent": "tsx examples/ai-agent-workflow.ts",
    "example:modify": "tsx examples/modify-workflow.ts",
    "generate": "tsx scripts/generate-node-types.ts",
    "playground": "tsx playground/index.ts"
  },
  "devDependencies": {
    "@n8n/eslint-config": "workspace:*",
    "@n8n/typescript-config": "workspace:*",
    "@n8n/vitest-config": "workspace:*",
    "@testing-library/jest-dom": "catalog:frontend",
    "@testing-library/user-event": "catalog:frontend",
    "@testing-library/vue": "catalog:frontend",
    "n8n-workflow": "workspace:*",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "license": "See LICENSE.md file in the root of the repository",
  "dependencies": {
    "prettier": "^3.6.2"
  }
}
