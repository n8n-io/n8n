{
  "name": "@n8n/data-transport",
  "version": "0.1.0",
  "description": "Data export and import tooling for n8n entities with encryption and database compatibility",
  "scripts": {
    "clean": "rimraf dist .turbo",
    "dev": "pnpm watch",
    "typecheck": "tsc --noEmit",
    "build": "tsc -p tsconfig.build.json",
    "format": "biome format --write .",
    "format:check": "biome ci .",
    "lint": "eslint . --quiet",
    "lint:fix": "eslint . --fix",
    "watch": "tsc -p tsconfig.build.json --watch",
    "test": "jest",
    "test:dev": "jest --watch"
  },
  "main": "dist/index.js",
  "module": "src/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "bin": {
    "n8n-data-transport": "./dist/cli/index.js"
  },
  "dependencies": {
    "commander": "catalog:",
    "fs-extra": "catalog:"
  },
  "devDependencies": {
    "@n8n/eslint-config": "workspace:*",
    "@n8n/typescript-config": "workspace:*",
    "@types/fs-extra": "catalog:"
  },
  "keywords": [
    "n8n",
    "data-migration",
    "export",
    "import",
    "database",
    "encryption"
  ],
  "license": "See LICENSE.md file in the root of the repository"
}
