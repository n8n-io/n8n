          "notes": "Wallet Config",
          "position": [
            640,
            600
          ],
          "parameters": {
            "fields": {
              "values": [
                {
                  "name": "Your Wallet Address",
                  "stringValue": "<Your TRC20 Wallet Address here>"
                },
