          "type": "n8n-nodes-base.filter",
          "notes": "Received in last 15m only",
          "position": [
            1060,
            600
          ],
          "parameters": {
            "conditions": {
              "number": [
                {
                  "value1": "={{ $json.Transfers.block_ts }}",
                  "value2": "={{ Date.now() - 900000 }}",
                  "operation": "smallerEqual"
