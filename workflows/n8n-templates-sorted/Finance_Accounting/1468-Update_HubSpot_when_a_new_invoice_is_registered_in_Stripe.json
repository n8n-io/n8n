            },
            "attachments": [
              {
                "color": "#FF3C00",
                "fields": {
                  "item": [
                    {
                      "short": true,
                      "title": "Amount",
                      "value": "={{$node[\"When Invoice Paid\"].json[\"data\"][\"object\"][\"amount_paid\"]/100}}"
                    },
                    {
                      "short": true,
                      "title": "Currency",
                      "value": "={{$node[\"When Invoice Paid\"].json[\"data\"][\"object\"][\"currency\"]}}"
                    },
                    {
                      "short": false,
                      "title": "Customer Name",
                      "value": "={{$node[\"When Invoice Paid\"].json[\"data\"][\"object\"][\"customer_name\"]}}"
