          ],
          "parameters": {
            "url": "https://api.twitter.com/1.1/account/update_profile_banner.json",
            "options": {},
            "requestMethod": "POST",
            "authentication": "oAuth1",
            "jsonParameters": true,
            "sendBinaryData": true,
            "binaryPropertyName": "banner:data"
          },
          "credentials": {
            "oAuth1Api": {
              "id": "300",
              "name": "Unnamed credential"
            }
          },
          "typeVersion": 1
        }
