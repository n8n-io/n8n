        {
          "id": "c4e7c596-4f2a-48e0-a932-ad40157c6282",
          "name": "Get interest name",
          "type": "n8n-nodes-base.telegramTrigger",
          "position": [
            100,
            660
          ],
          "webhookId": "3c85f114-6321-4e6d-9b5e-726e1493ee4e",
