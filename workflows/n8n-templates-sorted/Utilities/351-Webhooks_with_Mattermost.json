            "attachments": [
              {
                "image_url": "={{$node[\"HTTP Request\"].json[\"drinks\"][0][\"strDrinkThumb\"]}}"
              }
            ],
            "otherOptions": {}
          },
          "credentials": {
            "mattermostApi": "Mattermost"
          },
          "typeVersion": 1
        }
      ],
      "active": true,
      "settings": {},
