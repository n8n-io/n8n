                  "id": "91215336-3a47-4e86-ac6a-1a1862b31e54",
                  "name": "name",
                  "type": "string",
                  "value": "={{ $json.body.Nome.toTitleCase() }}"
                },
                {
                  "id": "703f1da3-3f68-4d97-94c9-c22661813d92",
                  "name": "email",
                  "type": "string",
                  "value": "={{ $json.body['E-mail'].toLowerCase() }}"
