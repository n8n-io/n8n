+function stringifyWords(o) {
+    if (o instanceof Word) {
+        return o.toString();
+    }
+    if (Array.isArray(o)) {
+        return o.map((oo) => stringifyWords(oo));
+    }
+    if (o && o.toString() == "[object Object]") {
+        return Object.fromEntries(Object.entries(o).map((oo) => [
+            stringifyWords(oo[0]),
+            stringifyWords(oo[1]),
+        ]));
+    }
+    return o;
+}
+// Special case that returns the parsed argument object
+function toParser(curl) {
+    const parserOutput = parse(curl);
