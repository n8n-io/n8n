# Example environment variables for n8n Shopify automation
# Copy to `.env` and fill with your credentials (do NOT commit secrets)

# Shopify (only needed if using API or verifying webhooks)
SHOPIFY_API_KEY=
SHOPIFY_PASSWORD=
SHOPIFY_SHARED_SECRET=

# Google Sheets
# Can be either path to a service account JSON or the JSON content
GOOGLE_SERVICE_ACCOUNT_JSON=
GOOGLE_SHEET_ID=

# SMTP (for sending customer emails and support fallback)
SMTP_HOST=
SMTP_PORT=587
SMTP_USER=
SMTP_PASS=
SMTP_FROM=

# Slack
SLACK_WEBHOOK_URL=

# Billing / QuickBooks (optional)
QUICKBOOKS_CLIENT_ID=
QUICKBOOKS_CLIENT_SECRET=

# Support / thresholds
SUPPORT_EMAIL=
HIGH_VALUE_THRESHOLD=200

# Optional: SFTP target for billing CSVs
BILLING_SFTP_HOST=
BILLING_SFTP_USER=
BILLING_SFTP_PASS=
BILLING_SFTP_BUCKET=





